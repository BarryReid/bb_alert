<?php

    include 'config.php';
    include 'include/snmp.php';
    include 'include/common.php';
    
    $arr = snmpTable(BB_SERVER, BB_SNMP_COMMUNITY_STRING, "1.3.6.1.4.1.3530.5.30.1.3");
    
    foreach($arr AS $key=>$value) {
        if (in_array('"BESTEST"', $value)) {
            return $key;
        }
    }
    
?>
